{
	"name": "LIVE support",
	"version": "1.2",
	"manifest_version": 2,
	"description": "LIVE support",
	"options_page": "options.html",
	"icons": {
		"16": "icon16.gif",
		"48": "icon48.gif"
	},
	"background": {
		"page": "background.html"
	},
	"browser_action": {
		"default_icon": "icon.gif"
	},
	"content_scripts": [
		{
			"matches": [
				"http://sp.pf.mbga.jp/12008305/*"
			],
			"js": [
				"jquery.min.js",
				"contentScript.js"
			],
			"run_at": "document_end"
		}
	]
}